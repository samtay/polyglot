- pattern: ^haskell/.*\.hs$
  commands:
  - cd haskell ; stack test
- pattern: ^python/.*\.py$
  commands:
  - cd python ; python -m doctest -v ${1:-**/*.py}
- pattern: ^ruby/.*\.rb$
  commands:
  - cd ruby ; bundle exec rake yard:doctest
- pattern: ^rust/.*\.rs$
  commands:
  - cd rust ; cargo test
